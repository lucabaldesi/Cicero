<%= render 'header' %>

<%= form_tag("/users", method: "post") do %>
	  <p>
    <%= label_tag(:name, "Your nickname: ") %><br>
    <%= text_field_tag(:name) %>
  </p>
 
    <p>
      <%= label_tag(:password, "Password: ") %><br>
      <%= password_field_tag(:password) %>
    </p>
   
      <p>
        <%= submit_tag("Submit") %>
      </p>
<% end %>
