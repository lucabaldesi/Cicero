<%= render "messages" %>

<%= form_tag("/users", method: "post") do %>
	  <p>
    <%= label_tag(:email, "Email: ") %><br>
    <%= text_field_tag(:email) %>
  </p>
 
    <p>
      <%= label_tag(:password, "Password: ") %><br>
      <%= password_field_tag(:password) %>
    </p>
   
      <p>
        <%= submit_tag("Submit") %>
      </p>
<% end %>
